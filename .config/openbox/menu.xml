<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu execute="openbox-xdgmenu /etc/xdg/menus/lxde-applications.menu" id="xdg-menu" label="Apps"/>
	<menu id="root-menu" label="Openbox 3">
		<item label="MousePad">
			<action name="Execute">
				<execute>
					mousepad
				</execute>
			</action>
		</item>
		<item label="Firefox">
			<action name="Execute">
				<execute>
					firefox
				</execute>
			</action>
		</item>
		<item label="GoogleChrome">
			<action name="Execute">
				<execute>
					google-chrome
				</execute>
			</action>
		</item>
		<separator/>
		<menu id="xdg-menu"/>
		<separator/>
		<menu id="root-menu-04656" label="Openbox">
			<item label="ObConf">
				<action name="Execute">
					<execute>
						obconf
					</execute>
				</action>
			</item>
			<item label="ObMenu">
				<action name="Execute">
					<execute>
						obmenu
					</execute>
				</action>
			</item>
			<item label="ObApps">
				<action name="Execute">
					<execute>
						obapps
					</execute>
				</action>
			</item>
			<item label="ObKey">
				<action name="Execute">
					<execute>
						obkey
					</execute>
				</action>
			</item>
			<item label="Reconfigure">
				<action name="Reconfigure"/>
			</item>
			<item label="Restart">
				<action name="Restart"/>
			</item>
		</menu>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<menu id="root-menu-43514" label="Logout">
			<item label="Reboot">
				<action name="Execute">
					<execute>sudo reboot</execute>
				</action>
			</item>
			<item label="Suspend">
				<action name="Execute">
					<execute>sudo pm-suspend</execute>
				</action>
			</item>
			<separator/>
			<item label="Shutdown">
				<action name="Execute">
					<execute>sudo poweroff</execute>
				</action>
			</item>
			<item label="Exit">
				<action name="Exit"/>
			</item>
		</menu>
		<separator/>
		<item label="LockScreen">
			<action name="Execute">
				<execute>xautolock -locknow</execute>
			</action>
		</item>
	</menu>
</openbox_menu>
